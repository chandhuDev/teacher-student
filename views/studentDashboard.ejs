       <% if (locals.arrayOfNames) { %>
          <% for (let index=0;index<locals.length;index++) { %>
                
                    <div class="textfield">
                        <%=arrayOfNames[index]%>
                    </div>
                    
                     <hr>
                     
                
            <% } %>
            
         <% } %>
    </div>

</body>
</html>